<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd
                        http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	version="2.0">

	<portlet>
		<description>Test Serve Resource Basic</description>
		<portlet-name>TestServeResourceBasic</portlet-name>
		<display-name>TestServeResourceBasic</display-name>
		<portlet-class>net.guto.portlet.test.serveresource.TestServeResourceBasic</portlet-class>

		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<supported-locale>pt_BR</supported-locale>

		<portlet-info>
			<title>Test Serve Resource Basic</title>
			<short-title>TestServeResourceBasic</short-title>
			<keywords>portlet, serveResource</keywords>
		</portlet-info>
	</portlet>
</portlet-app>
